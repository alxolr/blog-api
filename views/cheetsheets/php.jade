extends ../layout

block content
	.container
		H1 PHP Cheetsheet v1.0
		hr
		.row
			.col-sm-4
				.col-sm-12
					.panel.panel-info
						.panel-heading
							h4.board-card-title &nbsp;String Functions
						.panel-body
							ul
							each obj in cheet.strings
								ul
									li
										a(href='#{obj.doc}') #{obj.method}
										i  - #{obj.title}
			.col-sm-4
				.col-sm-12
					.panel.panel-danger
						.panel-heading
							h4.board-card-title &nbsp;HTTP Functions
						.panel-body
							ul
							each obj in cheet.http
								ul
									li
										a(href='#{obj.doc}') #{obj.method}
										i  - #{obj.title}
				.col-sm-12
					.panel.panel-success
						.panel-heading
							h4.board-card-title &nbsp;Directory Functions
						.panel-body
							ul
							each obj in cheet.directory
								ul
									li
										a(href='#{obj.doc}') #{obj.method}
										i  - #{obj.title}
				.col-sm-12
					.panel.panel-warning
						.panel-heading
							h4.board-card-title &nbsp;Variables Functions
						.panel-body
							ul
							each obj in cheet.variables
								ul
									li
										a(href='#{obj.doc}') #{obj.method}
										i  - #{obj.title}
			.col-sm-4
				.col-sm-12
					.panel.panel-primary
						.panel-heading
							h4.board-card-title &nbsp;Array Functions
						.panel-body
							ul
							each obj in cheet.arrays
								ul
									li
										a(href='#{obj.doc}') #{obj.method}
										i  - #{obj.title}